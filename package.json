{
  "name": "next-sql",
  "version": "1.0.2",
  "description": "Next-gen SQL connector, easy way to query SQL and create relationship linked object.",
  "keywords": [
    "sql",
    "mysql",
    "mysql2",
    "kenx",
    "query",
    "querybuilder",
    "connector",
    "db",
    "database",
    "database-js",
    "rds",
    "serverless"
  ],
  "author": "Cowboy Ho <info@cow-boy.net>",
  "license": "Apache-2.0",
  "types": "index.d.ts",
  "main": "index.js",
  "files": [
    "lib/**",
    "clients/**",
    "index.js",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/cow258/next-sql"
  },
  "scripts": {
    "dev": "node ./cli/dev",
    "cleanup": "rm -rf node_modules && rm yarn.lock",
    "lint:staged": "lint-staged",
    "lint:js": "eslint --ext .js .",
    "lint:fix": "eslint --ext .js --fix .",
    "test": "jest --coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "cli": "node ./cli",
    "format": "prettier --write \"**/*.d.ts\" --loglevel silent",
    "types": "yarn cli types clean && tsc --declaration --emitDeclarationOnly",
    "types:clean": "yarn cli types clean",
    "db:up": "docker-compose up -d",
    "db:start": "pnpm db:up && pnpm cli dbCheck",
    "db:down": "docker-compose down && pnpm db:clean",
    "db:stop": "docker-compose stop",
    "db:clean": "rm -rf db/datadir",
    "db:reset": "pnpm db:down && pnpm db:clean && pnpm db:up && pnpm cli dbCheck"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "classnames": "^2.5.1",
    "sqlstring": "^2.3.3"
  },
  "devDependencies": {
    "@planetscale/database": "^1.18.0",
    "@types/jest": "^26.0.24",
    "@types/mysql": "^2.15.26",
    "babel-eslint": "^10.1.0",
    "console.table": "^0.10.0",
    "coveralls-next": "^4.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^24.7.0",
    "glob": "^8.1.0",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.7",
    "mysqldump": "^3.2.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@planetscale/database": "^1.6.0",
    "mysql": "^2.0.0",
    "mysql2": "^3.9.7"
  },
  "peerDependenciesMeta": {
    "mysql": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "@planetscale/database": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,css,scss,yaml,yml}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --ext .js --fix ."
    ]
  }
}
